{
    "info": {
        "name": "Blood Donation System API",
        "description": "Postman collection for Blood Donation System APIs",
        "version": "1.1.0",
        "_postman_id": "unique-identifier-here",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
